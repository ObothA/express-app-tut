{
  "name": "express-postgres-knex-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set NODE_ENV=dev && set PGDB_PASSWORD=general && DEBUG=express-postgres-knex-app:* node ./bin/www",
    "dev": "npm run migrations-dev && npm run dev-run",
    "dev-run": "NODE_ENV=development PGDB_PASSWORD=general DEBUG=express-tut:* ./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "dev-refresh-db": "npm run migrations-dev && seed-dev",
    "migrations-dev": "NODE_ENV=development PGDB_PASSWORD=general npx knex migrate:latest",
    "seed-dev": "npx knex seed:run"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "knex": "^3.1.0",
    "morgan": "~1.9.1",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
